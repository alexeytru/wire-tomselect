{
    "name": "rishadblack/wire-tomselect",
    "description": "Tom Select",
    "license": "MIT",
    "authors": [
        {
            "name": "S M Rishad",
            "email": "rishad.black@gmail.com",
            "homepage": "https://smrishad.com"
        }
    ],
    "homepage": "https://github.com/rishadblack/wire-tomselect",
    "keywords": [
        "Laravel",
        "WireTomselect"
    ],
    "version": "1.1.0",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rishadblack\\WireTomselect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rishadblack\\WireTomselect\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rishadblack\\WireTomselect\\WireTomselectServiceProvider"
            ],
            "aliases": {
                "WireTomselect": "Rishadblack\\WireTomselect\\Facades\\WireTomselect"
            }
        }
    }
}
